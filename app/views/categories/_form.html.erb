<%= form_for(@category) do |f| %>
 <% if @category.errors.any? %>
   <h2>Erros</h2>
   <ul>
     <% @category.errors.full_messages.each do |message| %>
     <li><%= message %></li>
     <% end %>
   </ul>

 <% end %>
  <div class="name">
   <%= f.label :name %>
   <%= f.text_field :name %>
  </div>
  <div class="thumburl">
   <%= f.label :thumburl %>
   <%= f.url_field :thumburl %> 
  </div>
  <div class="actions">
  <%= f.submit %>
  </div>
<% end %>